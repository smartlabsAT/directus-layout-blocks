{
  "name": "directus-extension-interface-layout-blocks",
  "version": "0.0.2-alpha",
  "description": "Universal visual layout interface for M2A relations in Directus (Alpha Version)",
  "main": "dist/index.js",
  "private": true,
  "directus:extension": {
    "type": "interface",
    "path": "dist/index.js",
    "source": "src/index.ts",
    "host": "^10.0.0 || ^11.0.0"
  },
  "scripts": {
    "build": "directus-extension build",
    "dev": "directus-extension build -w --no-minify",
    "dev-full": "directus-extension build --no-minify",
    "link": "directus-extension link",
    "test": "vitest",
    "test:watch": "vitest --watch"
  },
  "keywords": [
    "directus",
    "directus-extension",
    "directus-interface",
    "layout",
    "blocks",
    "m2a",
    "drag-drop"
  ],
  "author": "Smartlabs Development Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/smartlabsAT/directus-layout-blocks.git"
  },
  "bugs": {
    "url": "https://github.com/smartlabsAT/directus-layout-blocks/issues"
  },
  "homepage": "https://github.com/smartlabsAT/directus-layout-blocks#readme",
  "dependencies": {
    "@directus/extensions-sdk": "^11.0.0",
    "@directus/stores": "^1.0.2",
    "lodash-es": "^4.17.21",
    "vue": "^3.4.0",
    "vuedraggable": "^4.1.0"
  },
  "devDependencies": {
    "@directus/extensions": "^2.0.0",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^20.0.0",
    "@vue/compiler-sfc": "^3.4.0",
    "@vue/test-utils": "^2.4.0",
    "glob": "^11.0.3",
    "sass": "^1.69.0",
    "typescript": "^5.3.0",
    "vite": "^5.0.0",
    "vitest": "^1.0.0"
  }
}
